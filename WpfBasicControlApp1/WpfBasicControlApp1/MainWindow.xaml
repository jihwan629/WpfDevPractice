<Window x:Class="WpfBasicControlApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfBasicControlApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <!--Basic Control--><!--
        
        <TextBlock Text="TextBlock" />

        <TextBox Text="TextBox" />

        <ProgressBar Value="50" Width="60" Height="20"/>
        
        <Slider Value="5" Width="60" />

        <PasswordBox Password="Secret"/>-->

        
        <!--Multimedia Contro--><!--
        
        <Image Source="Resources/Img0.png" Height="200" />
        
        --><!--동영상 파일 설정 > 출력 디렉토리 > 새버전이면 복사--><!--
        <MediaElement Source="Resources/Media0.avi" Height="200"/>-->
        
    </StackPanel>
</Window>
